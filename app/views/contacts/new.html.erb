<h2>new contacts</h2>

<%= form_for :article do |f| %>
    <p>
        <%= f.label :email %></br>
        <%= f.text_field :email %>
    </p>

    <p>    
        <%= f.label :message %></br>
        <%= f.text_area :message %>        
    </p>


    <p>
        <%= f.submit 'Submit'%>
    </p>


<%end%>